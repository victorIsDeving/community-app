{
  "expo": {
    "name": "community",
    "slug": "community",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logocommunity.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logocommunity.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "permissions": [
      "CAMERA",
      "MEDIA_LIBRARY",
      "MEDIA_LIBRARY_WRITE_ONLY"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.community.myapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
